{
  "name": "@nuxtjs/critters",
  "version": "0.8.0",
  "description": "Critical CSS for Nuxt",
  "keywords": [
    "performance",
    "nuxt",
    "module",
    "nuxt-module"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/critters.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && simple-git-hooks",
    "lint": "eslint .",
    "prepack": "nuxt-module-build build",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "bumpp && pnpm publish",
    "test": "vitest run"
  },
  "dependencies": {
    "@nuxt/kit": "^3.14.1592",
    "beasties": "^0.2.0",
    "pathe": "^1.1.2",
    "ufo": "^1.5.4"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "0.7.2",
    "@nuxt/module-builder": "0.8.4",
    "@nuxt/test-utils": "3.15.1",
    "@nuxtjs/critters": "link:./",
    "@vitest/coverage-v8": "2.1.8",
    "bumpp": "9.9.0",
    "eslint": "9.16.0",
    "lint-staged": "15.2.11",
    "nuxt": "3.14.1592",
    "simple-git-hooks": "2.11.1",
    "vitest": "2.1.8"
  },
  "resolutions": {
    "@nuxt/schema": "3.14.1592",
    "@nuxtjs/critters": "link:./"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": [
      "npx eslint --fix"
    ]
  },
  "packageManager": "pnpm@9.15.0"
}
